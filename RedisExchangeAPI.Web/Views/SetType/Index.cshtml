@model HashSet<string>
@{
    ViewData["Title"] = "Index";
}

<form asp-action="Add" method="post" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <button type="submit">Kaydet</button>
</form>

@foreach (var item in Model)
{
    <h2>
        @item
        <a asp-controller="SetType" asp-action="Delete" asp-route-name="@item">Delete</a>
    </h2>
}       